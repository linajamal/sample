<odoo>
    <record id="view_res_config_settings_form" model="ir.ui.view">
        <field name="name">res.config.settings.form.inherit</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="base.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//block[@name='integration']" position="inside">
                <setting string="Google Maps API Token" help="Enter your Google Maps API token" id="google_maps_api_token">
                    <field name="google_maps_api_token"/>
                </setting>
            </xpath>
        </field>
    </record>
</odoo>
